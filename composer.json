{
  "name": "bapcat/persist",
  "description": "A unified, object-oriented way to store data across media",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/BapCat/Persist",
  "keywords": ["File", "System", "Persist", "Persistence", "Storage", "Medium", "Media"],
  "authors": [
    {
      "name": "Corey Frenette",
      "email": "corey@narwhunderful.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "7.4"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php" : "^7.4|^8.0",
    "bapcat/propifier": "^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "BapCat\\Persist\\": "src/"
    }
  }
}
